class Solution {
    public int[] runningSum(int[] nums) {
        int[] result = new int[nums.length];
        result[0] = nums[0];
        for (int i = 1; i < nums.length; i++) {
            result[i] = result[i - 1] + nums[i];
        }
        return result;
    }
}
class Solution {
    public int minStartValue(int[] nums) {
        int startValue = 1;
        while (true) {  
            int sum = startValue;
            boolean valid = true;
            for (int num : nums) {
                sum += num;
                if (sum < 1) {
                    valid = false;
                    break;  
                }
            }
            if (valid) {
                return startValue;
            }
            startValue++;  
        }
    }
}
