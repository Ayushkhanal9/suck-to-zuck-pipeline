class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer,Integer> mappy = new HashMap<>();
        for(int i =0; i<nums.length; i++){
            int remainder = target-nums[i];
            if (mappy.containsKey(remainder)){
                return new int[] {mappy.get(remainder),i};
            }
            mappy.put(nums[i],i); 
        }
        return new int[]{};

    }        
}
